<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox styleClass="header">
         <children>
            <Label styleClass="title" text="Vehicles" />
            <Label styleClass="subtitle" text="Manage your vehicles." />
         </children>
      </VBox>
   </top>
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="100.0" text="ID" />
          <TableColumn prefWidth="200.0" text="Make" />
          <TableColumn prefWidth="200.0" text="Model" />
            <TableColumn prefWidth="100.0" text="Year" />
        </columns>
      </TableView>
   </center>
</BorderPane>
